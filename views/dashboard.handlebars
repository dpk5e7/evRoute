<div class="vstack gap-2">
  <div class="row">
    <div class="col-12 p-2 mx-auto">
      <div class="card border-primary">
        <div class="card-header bg-primary text-bg-primary">
          <div class="row">
            <div class="col-6">
              My Fleet
            </div>
            <div class="col-6 text-end">
              <a
                class="btn btn-outline-light btn-sm text-bg-primary"
                href="/addEV"
              ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-car-front" viewBox="0 0 16 16">
              <path d="M4 9a1 1 0 1 1-2 0 1 1 0 0 1 2 0Zm10 0a1 1 0 1 1-2 0 1 1 0 0 1 2 0ZM6 8a1 1 0 0 0 0 2h4a1 1 0 1 0 0-2H6ZM4.862 4.276 3.906 6.19a.51.51 0 0 0 .497.731c.91-.073 2.35-.17 3.597-.17 1.247 0 2.688.097 3.597.17a.51.51 0 0 0 .497-.731l-.956-1.913A.5.5 0 0 0 10.691 4H5.309a.5.5 0 0 0-.447.276Z"/>
              <path fill-rule="evenodd" d="M2.52 3.515A2.5 2.5 0 0 1 4.82 2h6.362c1 0 1.904.596 2.298 1.515l.792 1.848c.075.175.21.319.38.404.5.25.855.715.965 1.262l.335 1.679c.033.161.049.325.049.49v.413c0 .814-.39 1.543-1 1.997V13.5a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1-.5-.5v-1.338c-1.292.048-2.745.088-4 .088s-2.708-.04-4-.088V13.5a.5.5 0 0 1-.5.5h-2a.5.5 0 0 1-.5-.5v-1.892c-.61-.454-1-1.183-1-1.997v-.413a2.5 2.5 0 0 1 .049-.49l.335-1.68c.11-.546.465-1.012.964-1.261a.807.807 0 0 0 .381-.404l.792-1.848ZM4.82 3a1.5 1.5 0 0 0-1.379.91l-.792 1.847a1.8 1.8 0 0 1-.853.904.807.807 0 0 0-.43.564L1.03 8.904a1.5 1.5 0 0 0-.03.294v.413c0 .796.62 1.448 1.408 1.484 1.555.07 3.786.155 5.592.155 1.806 0 4.037-.084 5.592-.155A1.479 1.479 0 0 0 15 9.611v-.413c0-.099-.01-.197-.03-.294l-.335-1.68a.807.807 0 0 0-.43-.563 1.807 1.807 0 0 1-.853-.904l-.792-1.848A1.5 1.5 0 0 0 11.18 3H4.82Z"/>
              </svg> Add Vehicle</a>
            </div>
          </div>
        </div>
        <div class="table-responsive">
          <table class="table bg-light table-striped">
            <thead>
              <tr>
                <th scope="col">Model Year</th>
                <th scope="col">Manufacturer</th>
                <th scope="col">Model</th>
                <th scope="col">Category</th>
                <th scope="col">Drivetrain</th>
                <th scope="col">Engine Type</th>
                <th scope="col">Engine Size</th>
                {{! <th scope="col">Fuel</th> }}
                <th scope="col">Electric Range</th>
                <th scope="col">Fuel Economy Combined</th>
                <th scope="col">Fuel Economy City</th>
                <th scope="col">Fuel Economy Highway</th>
                <th scope="col">Photo</th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              {{#each vehicles as |vehicle|}}
                {{#with electric_vehicle}}
                  <tr>
                    <td scope="row">{{model_year}}</td>
                    <td>{{manufacturer_name}}</td>
                    <td>{{model}}</td>
                    <td>{{category_name}}</td>
                    <td>{{drivetrain}}</td>
                    <td>{{engine_type}}</td>
                    <td>{{engine_size}}</td>
                    {{!-- <td>{{fuel_name}}</td> --}}
                    <td>{{electric_range}}</td>
                    <td>{{alternative_fuel_economy_combined}}</td>
                    <td>{{alternative_fuel_economy_city}}</td>
                    <td>{{alternative_fuel_economy_highway}}</td>
                    <td>
                      {{#if manufacturer_url}}
                        <a href="{{manufacturer_url}}" target="_blank">
                          {{#if photo_url}}
                            <img
                              src="{{format_nrel_photo photo_url}}"
                              alt="{{model}}"
                              width="150"
                              height="80"
                              class="rounded"
                            />
                          {{else}}
                            {{manufacturer_name}}
                          {{/if}}</a>
                      {{else}}
                        {{#if photo_url}}
                          <img
                            src="{{format_nrel_photo photo_url}}"
                            alt="{{model}}"
                            width="150"
                            height="80"
                            class="rounded"
                          />
                        {{/if}}
                      {{/if}}
                    </td>
                    <td><a
                        href="/deleteEVFromFleet/{{id}}"
                        class="btn btn-danger text-bg-danger btn-sm"
                      ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                      <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z"/>
                      <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z"/>
                      </svg> Delete</a></td>
                  </tr>
                {{/with}}
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-12 p-2 mx-auto">
      <div class="card border-primary">
        <div class="card-header bg-primary text-bg-primary">
          <div class="row">
            <div class="col-6">
              My Trips
            </div>
            <div class="col-6 text-end">
              <a class="btn btn-outline-light btn-sm text-bg-primary"
                href="/trip"><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-map" viewBox="0 0 16 16">
              <path fill-rule="evenodd" d="M15.817.113A.5.5 0 0 1 16 .5v14a.5.5 0 0 1-.402.49l-5 1a.502.502 0 0 1-.196 0L5.5 15.01l-4.902.98A.5.5 0 0 1 0 15.5v-14a.5.5 0 0 1 .402-.49l5-1a.5.5 0 0 1 .196 0L10.5.99l4.902-.98a.5.5 0 0 1 .415.103zM10 1.91l-4-.8v12.98l4 .8V1.91zm1 12.98 4-.8V1.11l-4 .8v12.98zm-6-.8V1.11l-4 .8v12.98l4-.8z"/>
              </svg> New Trip</a>
            </div>
          </div>
        </div>
        <div class="table-responsive">
          <table class="table bg-light table-striped">
            <thead>
              <tr>
                <th scope="col">Title</th>
                <th scope="col">Start</th>
                <th scope="col">Destination</th>
                <th scope="col">Electric Vehicle</th>
                <th></th>
                <th></th>
              </tr>
            </thead>
            <tbody>
              {{#each trips as |trip|}}
                <tr>
                  <td>{{title}}</td>
                  <td>{{source_address}}</td>
                  <td>{{destination_address}}</td>
                  <td>
                    {{#with electric_vehicle}}
                      {{model_year}}
                      {{manufacturer_name}}
                      {{model}}
                    {{/with}}
                  </td>
                  <td><a
                      href="/trip/{{id}}"
                      class="btn btn-info text-bg-info btn-sm"
                    ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-map" viewBox="0 0 16 16">
                    <path fill-rule="evenodd" d="M15.817.113A.5.5 0 0 1 16 .5v14a.5.5 0 0 1-.402.49l-5 1a.502.502 0 0 1-.196 0L5.5 15.01l-4.902.98A.5.5 0 0 1 0 15.5v-14a.5.5 0 0 1 .402-.49l5-1a.5.5 0 0 1 .196 0L10.5.99l4.902-.98a.5.5 0 0 1 .415.103zM10 1.91l-4-.8v12.98l4 .8V1.91zm1 12.98 4-.8V1.11l-4 .8v12.98zm-6-.8V1.11l-4 .8v12.98l4-.8z"/>
                    </svg> View</a></td>
                  <td><a
                      href="/deleteTrip/{{id}}"
                      class="btn btn-danger text-bg-danger btn-sm"
                    ><svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-trash" viewBox="0 0 16 16">
                    <path d="M5.5 5.5A.5.5 0 0 1 6 6v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm2.5 0a.5.5 0 0 1 .5.5v6a.5.5 0 0 1-1 0V6a.5.5 0 0 1 .5-.5zm3 .5a.5.5 0 0 0-1 0v6a.5.5 0 0 0 1 0V6z"/>
                    <path fill-rule="evenodd" d="M14.5 3a1 1 0 0 1-1 1H13v9a2 2 0 0 1-2 2H5a2 2 0 0 1-2-2V4h-.5a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1H6a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1h3.5a1 1 0 0 1 1 1v1zM4.118 4 4 4.059V13a1 1 0 0 0 1 1h6a1 1 0 0 0 1-1V4.059L11.882 4H4.118zM2.5 3V2h11v1h-11z"/>
                    </svg> Delete</a></td>
                </tr>
              {{/each}}
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>